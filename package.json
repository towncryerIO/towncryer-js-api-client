{
  "name": "@towncryerio/towncryer-js-api-client",
  "version": "1.0.0-46b4174",
  "description": "OpenAPI client for @towncryerio/towncryer-js-api-client",
  "author": "OpenAPI-Generator Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/towncryerIO/towncryer-js-api-client.git"
  },
  "keywords": [
    "axios",
    "typescript",
    "openapi-client",
    "openapi-generator",
    "@towncryerio/towncryer-js-api-client"
  ],
  "license": "Unlicense",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "module": "./dist/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build:cjs": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:umd": "esbuild ./index.ts --bundle --global-name=towncryerjsapiclient --outfile=dist/towncryer-js-api-client.js --format=iife",
    "build:umd:min": "esbuild ./index.ts --bundle --global-name=towncryerjsapiclient --outfile=dist/towncryer-js-api-client.min.js --format=iife --minify",
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:umd:min",
    "prepare": "npm run build"
  },
  "dependencies": {
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.4",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/node": "12.11.5 - 12.20.42",
    "esbuild": "^0.25.9",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.0 || ^5.0"
  }
}
